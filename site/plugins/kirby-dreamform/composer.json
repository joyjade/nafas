{
	"name": "tobimori/kirby-dreamform",
	"description": "Kirby DreamForm is an opiniated form builder plugin for Kirby CMS 4+ that makes forms work like magic",
	"type": "kirby-plugin",
	"license": "proprietary",
	"homepage": "https://plugins.andkindness.com/dreamform",
	"version": "2.0.0",
	"authors": [
		{
			"name": "Tobias MÃ¶ritz",
			"email": "tobias@moeritz.io"
		}
	],
	"autoload": {
		"psr-4": {
			"tobimori\\DreamForm\\": "classes"
		}
	},
	"minimum-stability": "RC",
	"require": {
		"php": ">=8.3.0",
		"getkirby/composer-installer": "^1.2.1"
	},
	"scripts": {
		"dist": "composer install --no-dev --optimize-autoloader",
		"fix": "php-cs-fixer fix"
	},
	"config": {
		"optimize-autoloader": true,
		"allow-plugins": {
			"getkirby/composer-installer": true
		}
	},
	"extra": {
		"kirby-cms-path": false
	},
	"require-dev": {
		"getkirby/cms": "^5"
	}
}
